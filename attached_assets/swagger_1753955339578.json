  /payments/payment-configs/{id}/:
    get:
      operationId: payments_payment-configs_read
      summary: ViewSet for managing payment configurations.
      description: |-
        Provides complete CRUD functionality:
        - list: Get all payment configurations
        - retrieve: Get a specific configuration by ID
        - create: Create a new configuration
        - update: Update an existing configuration
        - partial_update: Partially update a configuration
        - destroy: Delete a configuration (not recommended)

        Also provides a custom action:
        - active: Get only the currently active payment method
      parameters: []
      responses:
        '200':
          description: ''
          schema:
            $ref: '#/definitions/PaymentConfig'
      tags:
      - payments
    put:
