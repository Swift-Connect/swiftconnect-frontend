      parameters:
      - name: data
        in: body
        required: true
        schema:
          $ref: '#/definitions/PaymentConfig'
      responses:
        '200':
          description: ''
          schema:
            $ref: '#/definitions/PaymentConfig'
      tags:
      - payments
    delete:
